@model WorkFlowDynamic.DataEntityTypes.SchemeWorkFlowSet

@{
    ViewData["Title"] = "Create";
}

<h1>Créer un nouveau workFlow</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="SchemeName" class="control-label"></label>
                <input asp-for="SchemeName" class="form-control" />
                <span asp-validation-for="SchemeName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Enregistrer" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Retourner</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
